{
  "name": "c1-northstar-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "prisma generate && next build",
    "start": "next start",
    "test": "jest",
    "test:api": "jest src/__tests__/api --testEnvironment=node",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:accessibility": "jest --testNamePattern='accessibility'",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "docker:dev": "docker-compose up -d",
    "docker:prod": "docker-compose -f docker-compose.prod.yml up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:build": "docker build -t c1-northstar-app .",
    "deploy:azure": "./scripts/deploy.sh",
    "deploy:status": "./scripts/deploy.sh status",
    "migrate": "./scripts/migrate.sh",
    "seed": "./scripts/seed.sh",
    "workers:start": "node scripts/start-workers.js",
    "health:check": "curl -f http://localhost:3000/api/health || exit 1",
    "postinstall": "prisma generate"
  },
